{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://backendflask.vercel.app/$1"
        }
    ],
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}